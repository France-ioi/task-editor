    "selector": "$json"
  },

  {
    "jsonSubPath": "task.title",
    "selector": "#taskTitle"
  },
  {
    "jsonSubPath": "task.taskIntro",
    "selector": "#taskIntro"
  },
  {
    "jsonSubPath": "context",
    "selector": "[task.js$contextTypePlaceholder]"
  },
  {
    "jsonSubPath": "conceptViewer",
    "template": {
      "file": "task.js",
      "placeholder": "conceptViewerPlaceholder"
    }
  },

  {
    "jsonSubPath": "groupByCategory",
    "template": {
      "file": "task.js",
      "placeholder": "groupByCategoryPlaceholder"
    }
  },
  {
    "jsonSubPath": "generatedBlocks",
    "template": {
      "file": "task.js",
      "placeholder": "generatedBlocksPlaceholder"
