  },
  {
    "jsonSubPath": "standardBlocks.includeAll",
    "template": {
      "file": "task.js",
      "placeholder": "includeAllPlaceholder"
    }
  },


  {
    "jsonSubPath": "standardBlocks.wholeCategories",
    "template": {
      "file": "task.js",
      "placeholder": "wholeCategoriesPlaceholder"
    },
    "keepArray": "true"
  },
  {
    "jsonSubPath": "standardBlocks.singleBlocks",
    "template": {
      "file": "task.js",
      "placeholder": "singleBlocksPlaceholder"
    },
    "keepArray": "true"
  },
  {
    "jsonSubPath": "difficulties",
    "matchingRule": [
      {
        "jsonSubPath": "scene.tiles",
        "template": {
          "file": "task.js",
          "placeholder": [ "tilesEasy", "tilesMedium", "tilesHard" ]
        }
      },
      {
        "jsonSubPath": "scene.images",
        "template": {
          "file": "index.html",
          "placeholder": ["images"],
          "json": "true"
        }
      },
      {
        "jsonSubPath": "scene.initItems",
        "template": {
          "file": "task.js",
          "placeholder": [ "initItemsEasy", "initItemsMedium", "initItemsHard" ]
        }
      },
      {
        "jsonSubPath": "name",
        "template": {
          "file": "task.js",
          "placeholder": [ "titleEasy", "titleMedium", "titleHard" ]
        }
      },
      {
        "jsonSubPath": "name",
        "selector": ["#testname", ".othertest", "#hardname"]
      },
      {
        "jsonSubPath": "gridInfo.maxInstructions",
        "template": {
          "file": "task.js",
          "placeholder": ["maxInstructionsEasy","maxInstructionsMedium","maxInstructionsHard"]
        }
      },

      {
        "jsonSubPath": "displayHelperThreshold",
        "template": {
          "file": "task.js",
          "placeholder": ["thresholdEasy","thresholdMedium","thresholdHard"]
        }
      }


    ]
  }

]
