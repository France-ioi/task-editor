[
  {
    "jsonSubPath": "title",
    "selector": "title"
  },
  {
    "jsonSubPath": "task",
    "selector": "#task"
  },
  {
    "jsonSubPath": "hints",
    "selector": ".hint"
  },
  {
    "jsonSubPath": "solution",
    "selector": "#solution"
  },
  {
    "jsonSubPath": "PEMTaskMetaData",
    "selector": "$PEMTaskMetaData"
  },
  {
    "jsonSubPath": "AlgoreaTrainingTaskMetaData",
    "selector": "$json"
  },
  {
    "jsonSubPath": "taskSettings",
    "selector": "[taskSettings.json]"
  },
  {
    "jsonSubPath": "context",
    "selector": "[task.js$contextTypePlaceholder]"
  },
  {
    "jsonSubPath": "difficulties",
    "matchingRule": [
      {
        "jsonSubPath": "scene.tiles",
        "template": {
          "file": "task.js",
          "placeholder": [ "tilesEasy", "tilesMedium", "tilesHard" ]
        }
      },
      {
        "jsonSubPath": "scene.initItems",
        "template": {
          "file": "task.js",
          "placeholder": [ "initItemsEasy", "nitItemsMedium", "initItemsHard" ]
        }
      },
      {
        "jsonSubPath": "name",
        "template": {
          "file": "task.js",
          "placeholder": [ "titleEasy", "titleMedium", "titleHard" ]
        }
      },
      {
        "jsonSubPath": "name",
        "selector": ["#testname", ".othertest", "#hardname"]
      }
    ]
  },
  {
    "jsonSubPath": "testFiles.input",
    "value": {
      "type": "file",
      "format": "tests/files/test[index].in"
    }
  },
  {
    "jsonSubPath": "testFiles.output",
    "value": {
      "type": "file",
      "format": "tests/files/test[index].out"
    }
  }
]
